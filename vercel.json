{
    "version": 2,
    "builds": [
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/",
            "dest": "/api"
        }
    ],
    "env": {
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}